doctype html
html 
     head 
         title= `${title}`
         meta(name="viewport" content="width=device-width, initial-scale=1")
         link(rel="stylesheet", href="/css/style.css")
     body    
        h1 Custom Object Table
        a(href="/update-cobj") Add/Update to this table 
        table(border='1')
          tr
            th.colored-header
              | Name
            th.colored-header
              | Alliance
            th.colored-header
              | Archenemy
            each superhero in data
              tr
                td #{superhero.properties.superhero_name}
                td #{superhero.properties.superhero_alliance}
                td #{superhero.properties.superhero_archenemy}